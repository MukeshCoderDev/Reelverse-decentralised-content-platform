{
  "name": "decentralized-adult-platform-subgraph",
  "version": "1.0.0",
  "description": "The Graph subgraph for decentralized adult platform analytics",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ decentralized-adult-platform",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 decentralized-adult-platform",
    "create-local": "graph create --node http://localhost:8020/ decentralized-adult-platform",
    "remove-local": "graph remove --node http://localhost:8020/ decentralized-adult-platform",
    "test": "graph test",
    "prepare:polygon": "mustache config/polygon.json subgraph.template.yaml > subgraph.yaml"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.56.0",
    "@graphprotocol/graph-ts": "0.30.0"
  },
  "devDependencies": {
    "mustache": "^4.2.0"
  }
}